{
  "name": "star-wars-dahboard",
  "author": "Alice R",
  "version": "0.0.1",
  "description": "A starter dashboard for AngularJS",
  "repository": "https://github.com/eponae/start-wars-dashboard",
  "license": "MIT",
  "devDependencies": {
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3",
    "extract-text-webpack-plugin": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "sasslint-webpack-plugin": "1.0.4",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-eslint": "8.0.1",
    "eslint": "4.9.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-config-airbnb-base": "12.1.0",
    "prettier-eslint-cli": "4.4.0",
    "html-loader": "0.5.1",
    "css-loader": "0.28.7",
    "style-loader": "0.19.0",
    "sass": "1.0.0-beta.2",
    "sass-lint": "1.12.1",
    "node-sass": "4.5.3",
    "sass-loader": "6.0.6",
    "file-loader": "1.1.5",
    "karma": "1.7.1",
    "karma-jasmine": "1.1.0",
    "karma-chrome-launcher": "2.2.0",
    "jasmine-core": "2.8.0"
  },
  "scripts": {
    "prestart": "npm install",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "format": "prettier-eslint --write \"app/scripts/**/*.js\"",
    "clean": "rm -fr ./dist && mkdir ./dist",
    "copy-fonts": "mkdir -p ./dist/fonts && cp ./node_modules/material-design-icons/iconfont/MaterialIcons-Regular.* ./dist/fonts/",
    "copy": "npm run copy-fonts",
    "prebuild": "npm run clean && npm run copy && npm run format",
    "start": "npm run prebuild && webpack-dev-server --port 4242 --hot --content-base dist/",
    "build": "PROD=true webpack -p"
  },
  "dependencies": {
    "angular": "1.6.6",
    "angular-animate": "1.6.6",
    "angular-aria": "1.6.6",
    "angular-messages": "1.6.6",
    "angular-ui-router": "0.4.3",
    "angular-material": "1.1.5",
    "material-design-icons": "3.0.1"
  }
}
